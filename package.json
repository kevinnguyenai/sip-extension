{
  "name": "oncxsip",
  "version": "0.0.1",
  "description": "oncx sip extension",
  "scripts": {
    "dev": "webextension-toolbox dev",
    "dev:chrome": "cross-env NODE_ENV=development yarn dev chrome",
    "dev:firefox": "cross-env NODE_ENV=development yarn dev firefox",
    "build": "webextension-toolbox build",
    "build:config": "webextension-toolbox build --config ./webextension-toolbox.config.js",
    "lint": "npm run lint-eslint",
    "tsc": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "prettier": "prettier -c --write \"**/**\""
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint -c .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint   --fix"
    ],
    "*.{ts,tsx,json,js,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "7.11.2",
    "@types/lodash": "^4.14.180",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "customize-cra": "^1.0.0",
    "dequal": "^2.0.2",
    "dotenv": "^16.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^3.13.1",
    "less-loader": "5.0.0",
    "less-vars-to-js": "^1.3.0",
    "loadable-components": "^2.2.3",
    "modernizr-webpack-plugin": "^1.0.7",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.6.0",
    "prop-types": "^15.8.1",
    "react-app-rewired": "^2.2.1",
    "style-loader": "^3.3.1",
    "umi-plugin-react": "^1.15.8",
    "url-loader": "^4.1.1",
    "webextension-toolbox": "latest"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.42",
    "@ant-design/pro-descriptions": "^1.10.58",
    "@ant-design/pro-form": "^1.59.3",
    "@ant-design/pro-layout": "^6.34.6",
    "@ant-design/pro-table": "^2.68.3",
    "@ant-design/pro-utils": "^1.37.2",
    "@umijs/plugin-access": "^2.5.0",
    "@umijs/plugin-initial-state": "^2.4.0",
    "@umijs/plugin-layout": "^0.18.2",
    "antd": "4.16.13",
    "dumi": "^1.1.40",
    "extensionizer": "^1.0.1",
    "history": "^5.3.0",
    "jssip": "3.8.2",
    "path-browserify": "^1.0.1",
    "react": "17.0.0",
    "react-dev-inspector": "1.1.1",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.2",
    "umi": "^3.5.21",
    "umi-request": "^1.4.0",
    "uuid": "^8.3.2"
  }
}
